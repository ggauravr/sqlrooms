{
  "name": "@sqlrooms/svelte-room-store",
  "version": "0.1.0",
  "description": "Svelte store management for SQLRooms applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "svelte": "^5.0.0",
    "zod": "^3.22.4",
    "@sqlrooms/room-config": "workspace:*",
    "@sqlrooms/utils": "workspace:*"
  },
  "devDependencies": {
    "@sqlrooms/preset-typescript": "workspace:*",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
} 